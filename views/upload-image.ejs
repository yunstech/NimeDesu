<%- include('partials/header2') %>


<div class="container" style="margin: 100px auto 500px">
    <div class="row">
        <div class="col-sm-12 mx-auto">
            <form action="/upload-carousel" method="POST" enctype="multipart/form-data">
                <div class="form-group">
                    <label for="exampleFormControlInput1">Title :</label>
                    <input type="text" class="form-control" name="title" />
                </div>
                <div class="form-group">
                    <label for="exampleFormControlInput1">Deskripsi :</label>
                    <input type="text" class="form-control" name="deskripsi" />
                </div>
                <div class="form-group">
                    <label for="exampleFormControlFile1">Gambar Content</label>
                    <input type="file" class="form-control-file" name="imageContent" id="imageContent" />
                </div>
                <button type="submit" class="btn btn-primary">Submit</button>
            </form>
        </div>
    </div>
</div>


